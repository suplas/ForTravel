<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fortravel.ReservationMapper">
<insert id="reservation" parameterType="ReservationDTO">
    insert into reservation (username,phoneno,phoneno2,phoneno3)
    values (#{username},#{phoneno},#{phoneno2},#{phoneno3})
   </insert>


</mapper>